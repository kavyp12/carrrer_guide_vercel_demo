{
    "version": 2,
    "builds": [
      {
        "src": "API/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "API/**/*.py",
        "use": "@vercel/python"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)",
        "dest": "API/auth.js"
      },
      {
        "src": "/api/marks",
        "dest": "API/marks.js"
      },
      {
        "src": "/api/questionnaire/(.*)",
        "dest": "API/questionnaire.js"
      },
      {
        "src": "/api/submit-assessment",
        "dest": "API/gemini.py"
      },
      {
        "src": "/api/download-report/(.*)",
        "dest": "API/gemini.py"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ],
